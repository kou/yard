<% if object.has_tag?(:example) %>
  <div class="examples">
    <h3><%= _("Examples:") %></h3>
    <% object.tags(:example).each do |tag| %>
      <h4><%= htmlify_line(_("tag|#{tag.tag_name}|#{tag.name}")) %></h4>
      <pre class="example code"><%= html_syntax_highlight(_(tag.text)) %></pre>
    <% end %>
  </div>
<% end %>
